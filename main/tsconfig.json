{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "outDir": "../build",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6"
  }
}
